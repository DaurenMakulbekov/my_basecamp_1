<h1>Projects</h1>
<table class="table">
  <thead>
    <tr>
      <th>Project name</th>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
      <% if project.user == current_user %>
      <tr>
        <td><%= link_to project.name, project %></td>
      </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
<%= link_to "New Project", new_project_path, class:"btn btn-primary" %>
